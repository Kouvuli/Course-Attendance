<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbox" alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button mnemonicParsing="false" onAction="#addCSVFileHandler" text="Thêm từ file csv" />
      <Button mnemonicParsing="false" onAction="#addListHandler" text="Thêm danh sách" />
      <Button mnemonicParsing="false" onAction="#addOneHandler" text="Thêm một" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
